<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?><?import javafx.scene.Cursor?><?import javafx.scene.control.Label?><?import javafx.scene.control.SplitPane?><?import javafx.scene.control.TableColumn?><?import javafx.scene.control.TableView?><?import javafx.scene.control.TextField?><?import javafx.scene.layout.AnchorPane?><?import javafx.scene.layout.VBox?>

<VBox minHeight="-Infinity" minWidth="-Infinity"  xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="com.example.smsroot2.LoginController">
    <children>
        <SplitPane maxHeight="50.0">
            <items>
                <AnchorPane maxHeight="50.0" minHeight="0.0" minWidth="0.0">
                    <children>
                        <TextField fx:id="edit_search" blendMode="SRC_ATOP" depthTest="ENABLE" promptText="内容"
                            AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                            AnchorPane.topAnchor="0.0">
                            <cursor>
                                <Cursor fx:constant="TEXT" />
                            </cursor>
                        </TextField>
                    </children>
                </AnchorPane>
            </items>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </SplitPane>
        <Label fx:id="size" text="总数量：" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" />
        <TableView fx:id="tableView">
            <columns>
                <TableColumn fx:id="id" prefWidth="50" text="id" />
                <TableColumn fx:id="tab_phone" prefWidth="150.0" text="设备号码" />
                <TableColumn fx:id="tab_ip" prefWidth="150.0" text="ip" />
                <TableColumn fx:id="tab_time" prefWidth="300.0" text="登录时间" />
            </columns>
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </VBox.margin>
        </TableView>
    </children>
    <padding>
        <Insets top="5.0" />
    </padding>
</VBox>
